<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Client.view.EndGameScene"
            stylesheets="@../CSS/SignUp.css"
            prefHeight="400.0" prefWidth="600.0">
    <center>
        <VBox alignment="CENTER" spacing="40">
            <Label fx:id="endGameLabel" style="-fx-background-color: darkgray; -fx-border-color: darkorange;" prefWidth="500" prefHeight="500"/>
            <Button text="Start a new Game" onMouseClicked="#startANewGame" maxWidth="200"/>
            <Button text="Back to main menu" onMouseClicked="#backToMainMenu" maxWidth="200"/>
        </VBox>
    </center>

</BorderPane>
